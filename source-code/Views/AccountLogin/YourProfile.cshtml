
@model WebApplication1.Models.AllLoggedUserInfo
@{
    ViewBag.Title = "YourProfile";
    Layout = "~/Views/Shared/_Admin_Layout.cshtml";
}

<h2>Your Profile</h2>

<h3>@Html.ActionLink("LOGIN INFO", "EditLogin", new { id = Model.user.Id })</h3>
<div class="form-group row">
    <label class="control-label col-md-2">Display Name :</label>
    <div class="col-md-10">
        @Model.user.display
    </div>
</div>

<div class="form-group row">
    <label class="control-label col-md-2">Username :</label>
    <div class="col-md-10">
        @Model.user.username
    </div>
</div>

<div class="form-group row">
    <label class="control-label col-md-2">Password :</label>
    <div class="col-md-10">
        @Model.user.password
    </div>
</div>

<div class="form-group row">
    <label class="control-label col-md-2">Status :</label>
    <div class="col-md-10">
        @Model.user.status
    </div>
</div>

<h3>@Html.ActionLink("ROLE", "EditRole", new { id=Model.user.Id})</h3>

<div class="form-group row">
    <label class="control-label col-md-2">Role :</label>
    <div class="col-md-10">
        @Model.role.RoleName
    </div>
</div>

<h3>@Html.ActionLink("BILLING & DELIVERY INFO", "EditInfo", new { id = Model.user.Id })</h3>

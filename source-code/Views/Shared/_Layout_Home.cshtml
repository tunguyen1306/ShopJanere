<script type="text/javascript">
    $(document).one('ready', function () {

        var procemessage = "<div value='0'> Loading Items...</div>";
        $("#bestseller").html(procemessage).show();
        //var url = "/Home/GetMetaGroup/";
        var url = "/Home/BestSeller/";
        $.ajax({
            url: url,
            data: { index: 1 },
            cache: false,
            type: "POST",
            success: function (data) {
                $("#bestseller").html(data).show();
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });
        var url = "/Home/MasterMetaGroup/";
        $.ajax({
            url: url,
            data: { index: 1 },
            cache: false,
            type: "POST",
            success: function (data) {
                $("#mastermeta").html(data).show();
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });

        /****************************

       MASTER META GROUP LIST

       ***************************/
        $("#mm_back").click(function () {

            var getMmCurr = $("#mm_cur").val();
            if (getMmCurr > 1) {
                getMmCurr--;
                $("#mm_cur").val(getMmCurr);
                var procemessage = "<div value='0'> Loading Items...</div>";
                $("#mastermeta").html(procemessage).show();
                //var url = "/Home/GetMetaGroup/";
                var url = "/Home/MasterMetaGroup/";
                $.ajax({
                    url: url,
                    data: { index: getMmCurr },
                    cache: false,
                    type: "POST",
                    success: function (data) {
                        $("#mastermeta").html(data).show();
                    },
                    error: function (reponse) {
                        alert("error : " + reponse);
                    }
                });
            }
        });
        $("#mm_next").click(function () {
            var getMnCurr = $("#mm_cur").val();
            getMnCurr++;
            $("#mm_cur").val(getMnCurr);
            var procemessage = "<div value='0'> Loading Items...</div>";
            $("#mastermeta").html(procemessage).show();
            var url = "/Home/MasterMetaGroup/";
            $.ajax({
                url: url,
                data: { index: getMnCurr },
                cache: false,
                type: "POST",
                success: function (data) {
                    if (!data.trim()) {
                        getMnCurr--;
                        $("#mm_cur").val(getMnCurr);
                        $.ajax({
                            url: url,
                            data: { index: getMnCurr },
                            cache: false,
                            type: "POST",
                            success: function (data) {
                                $("#mastermeta").html(data).show();
                            },
                            error: function (reponse) {
                                alert("error : " + reponse);
                            }
                        });
                    }
                    else {
                        $("#mastermeta").html(data).show();
                    }
                },
                error: function (reponse) {
                    alert("error : " + reponse);
                }
            });
        });

        /****************************

        BEST SELLER

        ***************************/
        $("#bs_back").click(function () {

            var getBsCurr = $("#bs_cur").val();
            if (getBsCurr > 1) {
                getBsCurr--;
                $("#bs_cur").val(getBsCurr);
                var procemessage = "<div value='0'> Loading Items...</div>";
                $("#bestseller").html(procemessage).show();
                //var url = "/Home/GetMetaGroup/";
                var url = "/Home/BestSeller/";
                $.ajax({
                    url: url,
                    data: { index: getBsCurr },
                    cache: false,
                    type: "POST",
                    success: function (data) {
                        $("#bestseller").html(data).show();
                    },
                    error: function (reponse) {
                        alert("error : " + reponse);
                    }
                });
            }
        });
        $("#bs_next").click(function () {
            var getBsCurr = $("#bs_cur").val();
            getBsCurr++;
            $("#bs_cur").val(getBsCurr);
            var procemessage = "<div value='0'> Loading Items...</div>";
            $("#bestseller").html(procemessage).show();
            var url = "/Home/BestSeller/";
            $.ajax({
                url: url,
                data: { index: getBsCurr },
                cache: false,
                type: "POST",
                success: function (data) {
                    if (!data.trim()) {
                        getBsCurr--;
                        $("#bs_cur").val(getBsCurr);
                        $.ajax({
                            url: url,
                            data: { index: getBsCurr },
                            cache: false,
                            type: "POST",
                            success: function (data) {
                                $("#bestseller").html(data).show();
                            },
                            error: function (reponse) {
                                alert("error : " + reponse);
                            }
                        });
                    }
                    else {
                        $("#bestseller").html(data).show();
                    }
                },
                error: function (reponse) {
                    alert("error : " + reponse);
                }
            });
        });
    });
</script>

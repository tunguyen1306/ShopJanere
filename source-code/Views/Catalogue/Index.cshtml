@model IEnumerable<WebApplication1.catalogue>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Admin_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CatalogueCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CatalogueName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MetaGroupId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CatalogueCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CatalogueName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MetaGroupId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

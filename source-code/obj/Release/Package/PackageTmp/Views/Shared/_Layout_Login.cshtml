<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color: #171717">
        <div class="container">
            <div class="navbar-header" style="width:100%">
                <div class="row">
                    <div class="col-lg-12">
                        <a href="@Url.Action("Index", "Home")"> <img src="~/Content/head/logo.png" style=" padding: 33px 0px;" /></a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container body-content">
        <div class="row">
            <div class="col-lg-12" style="padding:0px;">
                @RenderBody()
            </div>
        </div>



        <hr />

    </div>
    <div style="background-color: #235d91">
        <div class="container">
            <footer>
                <div class="row" style="padding: 10px 0px; text-align: center;">
                    <div class="col-lg-3">&nbsp;</div>
                    <div class="col-lg-2" style="padding: 10px 0px;">
                        <a href="tel:+372 658 0300"> <img src="~/Content/head/contact-phone.png" /><span style="color: white; ">+372 658 0300</span></a>

                    </div>
                    <div class="col-lg-2" style="padding: 10px 0px;">
                        <a href="mailto:janere@janere.ee"> <img src="~/Content/head/contact-email.png" /><span style="color: white; ">janere@janere.ee</span></a>
                    </div>
                    <div class="col-lg-1"><a href="https://www.facebook.com/janere.ee"><img src="~/Content/foot/facebook.png" /></a></div>
                    <div class="col-lg-1"><a href="skype:oujanere?chat"><img src="~/Content/foot/skype.png" /></a></div>
                    <div class="col-lg-3">&nbsp;</div>
                </div>
            </footer>
        </div>
    </div>
    <div style="background-color: #171717">
        <div class="container">
            <footer>
                <div class="row" style="padding: 10px 0px; text-align: center;">
                    <img src="~/Content/foot/line2.JPG" />
                </div>
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/myscript")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
